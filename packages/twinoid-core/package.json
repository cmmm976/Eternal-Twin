    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.2",
    "eslint": "^7.10.0",
    "kryo-json": "^0.11.0",
    "kryo-testing": "^0.11.0",
    "mocha": "^8.1.3",
    "typescript": "^4.0.3"
  },
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./lib/": "./lib/"
