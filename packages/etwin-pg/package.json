{
  "name": "@eternal-twin/etwin-pg",
  "version": "0.0.1",
  "homepage": "https://gitlab.com/eternal-twin/etwin",
  "description": "Postgres schema for Eternal-Twin",
  "repository": {
    "type": "git",
    "url": "git://gitlab.com:eternal-twin/etwin.git"
  },
  "private": false,
  "scripts": {
    "lint": "eslint \"./src/**/*.ts\"",
    "format": "eslint --fix \"./src/**/*.ts\"",
    "build": "tsc --build src/lib/tsconfig.json",
    "pretest": "tsc --build src/test/tsconfig.json",
    "test": "mocha \"test/**/*.spec.js\"",
    "prestart": "tsc --build src/main/tsconfig.json",
    "start": "node main/main.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "pre-commit": {
    "run": [
      "lint"
    ]
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "@eternal-twin/pg-db": "^0.0.1",
    "@types/pg": "^7.14.3",
    "furi": "^2.0.0",
    "pg": "^8.2.1"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.5",
    "dotenv": "^8.2.0",
    "eslint": "^7.1.0",
    "find-up": "^4.1.0",
    "mocha": "^7.2.0",
    "typescript": "^3.9.3"
  },
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./lib/": "./lib/"
  },
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "./lib/"
  ]
}
