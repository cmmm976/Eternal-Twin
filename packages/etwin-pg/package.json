  },
  "devDependencies": {
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.10.0",
    "find-up": "^5.0.0",
    "mocha": "^8.1.3",
    "typescript": "^4.0.3"
  },
  "type": "module",
  "exports": {
