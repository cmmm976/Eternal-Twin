{
  "name": "@eternal-twin/etwin-rest",
  "version": "0.0.1",
  "homepage": "https://gitlab.com/eternal-twin/etwin",
  "description": "Node.js in-memory client for the Eternal-Twin API",
  "repository": {
    "type": "git",
    "url": "git://gitlab.com:eternal-twin/etwin.git"
  },
  "private": false,
  "scripts": {
    "lint": "eslint \"./src/**/*.ts\"",
    "format": "eslint --fix \"./src/**/*.ts\"",
    "lib:build": "node_modules/.bin/tsc --build src/lib/tsconfig.json",
    "main:build": "node_modules/.bin/tsc --build src/main/tsconfig.json",
    "test:build": "node_modules/.bin/tsc --build src/test/tsconfig.json",
    "pretest": "yarn run test:build",
    "test": "mocha \"test/**/*.spec.js\"",
    "prestart": "yarn run main:build",
    "start": "node main/main.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "pre-commit": {
    "run": [
      "lint"
    ]
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "@eternal-twin/etwin-api-types": "*",
    "@types/koa": "^2.11.0",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-route": "^3.2.4",
    "koa": "^2.11.0",
    "koa-mount": "^4.0.0",
    "koa-route": "^3.2.0"
  },
  "devDependencies": {
    "@eternal-twin/etwin-api-in-memory": "*",
    "@koa/cors": "^3.0.0",
    "@types/koa__cors": "^3.0.1",
    "@types/koa-logger": "^3.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.5",
    "eslint": "^6.8.0",
    "koa-logger": "^3.2.1",
    "mocha": "^7.1.1",
    "typescript": "^3.8.3"
  },
  "type": "module",
  "exports": {
    "./lib/": "./lib/"
  },
  "files": [
    "./lib/"
  ]
}
